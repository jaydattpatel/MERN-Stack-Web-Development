
    <div class="container">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                </tr>
            </thead>
            <tbody>
                <% productsInHTML.forEach(product=>{ %>
                    <tr>
                        <th scope="row"><%= product.id %></th>
                        <td><%= product.name %></td>
                        <td><%= product.desc %> </td>
                        <td><%= product.price %></td>
                        <td>
                            <img src="<%= product.imageUrl %>" />
                        </td>
                    </tr>
               <% }) %> 
            </tbody>
        </table>
    </div>
